extends layouts/main
include mixins/videoBlock
block content
    .search__header
        h3 Searching by #{searchingBy}
    .search_videos
        if videos.length === 0
            h5 해당하는 영상을 찾지 못했습니다.
        each item in videos
            +videoBlock({
                title:item.title,
                views:item.views,
                videoFile:item.videoFile,
                id:item.id
            })